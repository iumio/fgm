route:
  name : iumio_manager_cache_manager
  path :  /_manager/cache
  activity : Cache%cache
endroute

route:
  name : iumio_manager_cache_manager_remove_all
  path :  /_manager/cache/clear/all
  activity : Cache%cacheClearAll
endroute

route:
  name : iumio_manager_cache_manager_remove
  path :  /_manager/cache/clear/{env}
  activity : Cache%cacheClear
endroute

route:
  name : iumio_manager_cache_manager_get_all
  path :  /_manager/cache/get/all
  activity : Cache%getAllEnv
endroute